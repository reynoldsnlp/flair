<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:m.html="urn:import:gwt.material.design.client.ui.html">
	<ui:style>
	
	</ui:style>
	
	<m:MaterialModal ui:field="mdlExporterUI" dismissible="true">
		<m:MaterialModalContent>
			<m:MaterialRow textAlign="CENTER">
				<m:MaterialTitle ui:field="lblTitleUI" title="Export Settings" 
					description="You can use this link to apply your current settings to further searches, or share the link with students so they can use the web search with your settings."/>
			</m:MaterialRow>
			
			<m:MaterialRow>
				<m:MaterialTextBox ui:field="txtURLUI" text="Exported_URL" readOnly="false"/>
			</m:MaterialRow>
		</m:MaterialModalContent>
		
		<m:MaterialModalFooter>
			<m:MaterialButton ui:field="btnCloseUI" type="FLAT" text="Close"/>
		</m:MaterialModalFooter>
	</m:MaterialModal>
</ui:UiBinder> 