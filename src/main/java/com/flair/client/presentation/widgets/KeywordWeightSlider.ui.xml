<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:m.html="urn:import:gwt.material.design.client.ui.html">
		
	<m:MaterialPanel marginTop="3">
		<m:MaterialRow marginBottom="2">
			<m:MaterialRow marginBottom="2">
				<m:MaterialColumn>
					<m:MaterialCheckBox ui:field="chkToggleUI" text="Academic Vocab"/>
				</m:MaterialColumn>
				
				<m:MaterialColumn textAlign="RIGHT">
					<m:MaterialIcon ui:field="btnEditKeywordsUI" iconType="MODE_EDIT" waves="DEFAULT" circle="true" iconColor="BLUE" marginRight="5"/>
					<m:MaterialIcon ui:field="btnResetKeywordsUI" iconType="UNDO" waves="DEFAULT" circle="true" iconColor="BLUE" marginLeft="5"/>
				</m:MaterialColumn>
			</m:MaterialRow>
			
			<m:MaterialRange ui:field="sldWeightUI"/>
		</m:MaterialRow>
	</m:MaterialPanel>
</ui:UiBinder> 