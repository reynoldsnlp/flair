<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:flair="urn:import:com.flair.client.presentation.widgets">
	
	<g:HTMLPanel ui:field="pnlRootUI">
		<flair:GrammaticalConstructionPanel accordion="true">
			<!-- Sätze -->
			<flair:GrammaticalConstructionPanelItem ui:field="pnlSaetzeUI" text="sätze" iconType="EXPAND_MORE">
				<flair:GrammaticalConstructionPanel accordion="true" popout="false">
					<!-- Fragen -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlFragenUI" text="fragen" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_DIRECT" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_WH" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_WHAT" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_WHEN" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_WHERE" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_WHICH" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_WHO" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_WHOM" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_WHOSE" parentLanguage="GERMAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_WHY" parentLanguage="GERMAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_HOW" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_YESNO" parentLanguage="GERMAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_TAG" parentLanguage="GERMAN"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Satztypen -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlSatztypenUI" text="sentence types" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="SENTENCE_SIMPLE" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="SENTENCE_COMPOUND" parentLanguage="GERMAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="SENTENCE_COMPLEX" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="CLAUSE_SUBORDINATE" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="SENTENCE_INCOMPLETE" parentLanguage="GERMAN"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Nebensatztypen -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlNebensatztypenUI" text="nebensatztypen" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="CLAUSE_RELATIVE" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="CLAUSE_ADVERBIAL" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="CLAUSE_DASS" parentLanguage="GERMAN"/>
					</flair:GrammaticalConstructionPanelItem>
				</flair:GrammaticalConstructionPanel>
			</flair:GrammaticalConstructionPanelItem>
		
			<!-- Wortarten -->
			<flair:GrammaticalConstructionPanelItem ui:field="pnlWortartenUI" text="wortarten" iconType="EXPAND_MORE">
				<flair:GrammaticalConstructionPanel accordion="true" popout="false">
					<!-- Verben -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlVerbenUI" text="verben" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionPanel accordion="true" popout="false">
							<!-- Verbtypen -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlVerbtypenUI" text="verbtypen" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="VERBTYP_MAIN" parentLanguage="GERMAN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="VERBTYP_AUXILIARIES" parentLanguage="GERMAN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="VERBTYP_MODAL" parentLanguage="GERMAN"/>
							</flair:GrammaticalConstructionPanelItem>			
			
							<!-- Zeiten -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlZeitenUI" text="zeiten" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_PRESENT_PERFECT_HABEN" parentLanguage="GERMAN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_PRESENT_PERFECT_SEIN" parentLanguage="GERMAN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_PAST_PERFECT_HABEN" parentLanguage="GERMAN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_PAST_PERFECT_SEIN" parentLanguage="GERMAN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_FUTURE_SIMPLE" parentLanguage="GERMAN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_FUTURE_PERFECT" parentLanguage="GERMAN"/>
							</flair:GrammaticalConstructionPanelItem>
							
							<!-- Passiv -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlPassivUI" text="passiv" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="PASSIVE_VOICE_WERDEN" parentLanguage="GERMAN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="PASSIVE_VOICE_SEIN" parentLanguage="GERMAN"/>
							</flair:GrammaticalConstructionPanelItem>
							
							<!-- Satzklammer -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlSatzklammerUI" text="satzklammer" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="VERB_BRACKETS" parentLanguage="GERMAN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="VERB_CLUSTER" parentLanguage="GERMAN"/>
							</flair:GrammaticalConstructionPanelItem>
							
							<!-- Verbformen -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlVerbformenUI" text="verbformen" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="IMPERATIVES" parentLanguage="GERMAN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="VERBFORM_INFINITIVE" parentLanguage="GERMAN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="VERBFORM_TO_INFINITIVE" parentLanguage="GERMAN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="VERBFORM_PARTICIPLE" parentLanguage="GERMAN"/>
							</flair:GrammaticalConstructionPanelItem>
						</flair:GrammaticalConstructionPanel>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Negation -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlNegationUI" text="negation" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="NEGATION_ALL" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="NEGATION_PARTIAL" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="NEGATION_NO_NOT_NEVER" parentLanguage="GERMAN"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Artikel -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlArtikelUI" text="artikel" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="ARTICLES" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ARTICLE_A" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ARTICLE_THE" parentLanguage="GERMAN"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Indefinite Quantifizierer -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlIndefiniteQuantifiziererUI" text="indefinite quantifizierer" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="DETERMINER_SOME" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="DETERMINER_ANY" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="DETERMINER_MANY" parentLanguage="GERMAN"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Adjektive, Adverben und Zahlworte -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlAdjAdvZWUI" text="adjektive, adverben und zahlworte" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="ADJECTIVE_POSITIVE" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ADVERB_POSITIVE" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="PARTICLE_PLUS_ADJ_ADV" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="CARDINALS" parentLanguage="GERMAN"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Pronomen -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlPronomenUI" text="pronomen" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="PRONOUNS" parentLanguage="GERMAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="PRONOUNS_PERSONAL" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="PRONOUNS_RELATIVE" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="PRONOUNS_POSSESSIVE" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="PRONOUNS_DEMONSTRATIVE" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="PRONOUNS_REFLEXIVE" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="PRONOUNS_INDEFINITE" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="PRONOUNS_INTERROGATIVE" parentLanguage="GERMAN"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Präpositionen -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlPraepositionenUI" text="präpositionen" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="PREPOSITIONS" parentLanguage="GERMAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="PREPOSITIONS_SIMPLE" parentLanguage="GERMAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="POSTPOSITION" parentLanguage="GERMAN"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Nomen -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlNomenUI" text="nomen" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionPanel accordion="true" popout="false">
							<!-- Nominalisierungen -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlNominalisierungenUI" text="nominalisierungen" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="NOUNS_ISMUS" parentLanguage="GERMAN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="NOUNS_TUR" parentLanguage="GERMAN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="NOUNS_UNG" parentLanguage="GERMAN"/>
							</flair:GrammaticalConstructionPanelItem>
							
							<!-- Attribute -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlAttributeUI" text="attribute" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="ATTRIBUTES_PARTICIPLE_1" parentLanguage="GERMAN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="ATTRIBUTES_PARTICIPLE_2" parentLanguage="GERMAN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="ATTRIBUTES_ADJECTIVE" parentLanguage="GERMAN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="ATTRIBUTES_PREPOSITION" parentLanguage="GERMAN"/>
							</flair:GrammaticalConstructionPanelItem>
						</flair:GrammaticalConstructionPanel>
					</flair:GrammaticalConstructionPanelItem>
				</flair:GrammaticalConstructionPanel>
			</flair:GrammaticalConstructionPanelItem>
		</flair:GrammaticalConstructionPanel>
	</g:HTMLPanel>
</ui:UiBinder> 