<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:flair="urn:import:com.flair.client.presentation.widgets">
	
	<g:HTMLPanel ui:field="pnlRootUI">
		<flair:GrammaticalConstructionPanel accordion="true">			
			<!-- Sentences -->
			<flair:GrammaticalConstructionPanelItem ui:field="pnlSentencesUI" text="sentences" iconType="EXPAND_MORE">
				<flair:GrammaticalConstructionPanel accordion="true" popout="false">
					<!-- Questions -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlQuestionsUI" text="questions" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_DIRECT" parentLanguage="RUSSIAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_WH" parentLanguage="RUSSIAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="QUESTIONS_YESNO" parentLanguage="RUSSIAN"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Sentence Types -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlSentenceTypesUI" text="sentence types" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="SENTENCE_SIMPLE" parentLanguage="RUSSIAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="CLAUSE_SUBORDINATE" parentLanguage="RUSSIAN"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Clause Types -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlClauseTypesUI" text="clause types" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="CLAUSE_RELATIVE" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="CONDITIONALS_UNREAL" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="EXISTENTIAL_THERE" parentLanguage="RUSSIAN"/>
					</flair:GrammaticalConstructionPanelItem>
				</flair:GrammaticalConstructionPanel>
			</flair:GrammaticalConstructionPanelItem>
			
			<!-- Parts of Speech -->
			<flair:GrammaticalConstructionPanelItem ui:field="pnlPartsOfSpeechUI" text="parts of speech" iconType="EXPAND_MORE">
				<flair:GrammaticalConstructionPanel accordion="true" popout="false">
					<!-- Verbs -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlVerbsUI" text="verbs" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionPanel accordion="true" popout="false">
							<!-- Verb Forms -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlVerbFormsUI" text="verb forms" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="VERBS_IRREGULAR" parentLanguage="RUSSIAN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="VERBS_REGULAR" parentLanguage="RUSSIAN"/>
							</flair:GrammaticalConstructionPanelItem>			
			
							<!-- Tenses -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlTensesUI" text="tenses" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_PAST" parentLanguage="RUSSIAN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_NON_PAST" parentLanguage="RUSSIAN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_PRESENT" parentLanguage="RUSSIAN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="TENSE_FUTURE" parentLanguage="RUSSIAN"/>
							</flair:GrammaticalConstructionPanelItem>
							
							<!-- Aspect -->
							<flair:GrammaticalConstructionPanelItem ui:field="pnlAspectUI" text="aspect" iconType="EXPAND_MORE">
								<flair:GrammaticalConstructionWeightSlider gram="ASPECT_PERFECTIVE" parentLanguage="RUSSIAN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="ASPECT_IMPERFECTIVE" parentLanguage="RUSSIAN"/>
								
								<flair:GrammaticalConstructionWeightSlider gram="ASPECT_BIASPECTUAL" parentLanguage="RUSSIAN"/>
							</flair:GrammaticalConstructionPanelItem>

						</flair:GrammaticalConstructionPanel>
					</flair:GrammaticalConstructionPanelItem>
				
					<!-- Negation -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlNegationUI" text="negation" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="NEGATION_ALL" parentLanguage="RUSSIAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="NEGATION_NO_NOT_NEVER" parentLanguage="RUSSIAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="NEGATION_NOT" parentLanguage="RUSSIAN"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Quantifiers -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlQuantifiersUI" text="quantifiers" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="DETERMINER_SOME" parentLanguage="RUSSIAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="DETERMINER_ANY" parentLanguage="RUSSIAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="DETERMINER_MUCH" parentLanguage="RUSSIAN"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Adjectives -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlAdjectivesUI" text="adjectives" iconType="EXPAND_MORE">
						<!--<flair:GrammaticalConstructionWeightSlider gram="ADJECTIVE_POSITIVE" parentLanguage="RUSSIAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ADJECTIVE_COMPARATIVE_SHORT" parentLanguage="RUSSIAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ADJECTIVE_SUPERLATIVE_SHORT" parentLanguage="RUSSIAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ADJECTIVE_COMPARATIVE_LONG" parentLanguage="RUSSIAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ADJECTIVE_SUPERLATIVE_LONG" parentLanguage="RUSSIAN"/>-->

						<flair:GrammaticalConstructionWeightSlider gram="ATTRIBUTES_ADJECTIVE" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="ADJECTIVE_NOMINATIVE" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="ADJECTIVE_ACCUSATIVE" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="ADJECTIVE_GENITIVE" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="ADJECTIVE_PREPOSITIONAL" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="ADJECTIVE_DATIVE" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="ADJECTIVE_INSTRUMENTAL" parentLanguage="RUSSIAN"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Adverbs -->
					<!--flair:GrammaticalConstructionPanelItem ui:field="pnlAdverbsUI" text="adverbs" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="ADVERB_POSITIVE" parentLanguage="RUSSIAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ADVERB_COMPARATIVE_SHORT" parentLanguage="RUSSIAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ADVERB_SUPERLATIVE_SHORT" parentLanguage="RUSSIAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ADVERB_COMPARATIVE_LONG" parentLanguage="RUSSIAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="ADVERB_SUPERLATIVE_LONG" parentLanguage="RUSSIAN"/>
					</flair:GrammaticalConstructionPanelItem-->
					
					<!-- Pronouns -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlPronounsUI" text="pronouns" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="PRONOUNS_POSSESSIVE" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="PRONOUNS_REFLEXIVE" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="PRONOUN_NOMINATIVE" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="PRONOUN_ACCUSATIVE" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="PRONOUN_GENITIVE" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="PRONOUN_PREPOSITIONAL" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="PRONOUN_DATIVE" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="PRONOUN_INSTRUMENTAL" parentLanguage="RUSSIAN"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Conjunctions -->
					<!--flair:GrammaticalConstructionPanelItem ui:field="pnlConjunctionsUI" text="conjunctions" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="CONJUNCTIONS_SIMPLE" parentLanguage="RUSSIAN"/>
						
						<flair:GrammaticalConstructionWeightSlider gram="CONJUNCTIONS_ADVANCED" parentLanguage="RUSSIAN"/>
					</flair:GrammaticalConstructionPanelItem-->
					
					<!-- Prepositions -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlPrepositionsUI" text="prepositions" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="PREPOSITIONS" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="PREPOSITION_NOMINATIVE" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="PREPOSITION_ACCUSATIVE" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="PREPOSITION_GENITIVE" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="PREPOSITION_PREPOSITIONAL" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="PREPOSITION_DATIVE" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="PREPOSITION_INSTRUMENTAL" parentLanguage="RUSSIAN"/>
					</flair:GrammaticalConstructionPanelItem>
					
					<!-- Nouns -->
					<flair:GrammaticalConstructionPanelItem ui:field="pnlNounsUI" text="nouns" iconType="EXPAND_MORE">
						<flair:GrammaticalConstructionWeightSlider gram="NOUN_NOMINATIVE" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="NOUN_ACCUSATIVE" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="NOUN_GENITIVE" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="NOUN_PREPOSITIONAL" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="NOUN_DATIVE" parentLanguage="RUSSIAN"/>

						<flair:GrammaticalConstructionWeightSlider gram="NOUN_INSTRUMENTAL" parentLanguage="RUSSIAN"/>
					</flair:GrammaticalConstructionPanelItem>
				</flair:GrammaticalConstructionPanel>
			</flair:GrammaticalConstructionPanelItem>	
		</flair:GrammaticalConstructionPanel>
	</g:HTMLPanel>
</ui:UiBinder> 