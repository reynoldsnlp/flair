<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:m.html="urn:import:gwt.material.design.client.ui.html">
	
	<ui:style>
	
	</ui:style>
	
	<m:MaterialModal ui:field="mdlPromptUI" type="BOTTOM_SHEET" dismissible="false">
		<m:MaterialModalContent marginTop="13">
			<m:MaterialTitle ui:field="lblTextUI" title="Title" description="Description"
				marginTop="13"/>
		</m:MaterialModalContent>
		
		<m:MaterialModalFooter>
			<m:MaterialButton ui:field="btnYesUI" type="FLAT" text="Yes"/>
			<m:MaterialButton ui:field="btnNoUI" type="FLAT" text="No"/>
		</m:MaterialModalFooter>
	</m:MaterialModal>
</ui:UiBinder> 